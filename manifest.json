{
  "name": "heart",
  "version": "0.1.0",
  "description": "1.禁止请求缓存",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "48": "img/icon_48.png",
    "64": "img/icon_64.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "browsingData",
    "<all_urls>",
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "management"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "清楚缓存并开关插件",
    "default_popup": "popup.html"
  },
  "persistrnt": false
}